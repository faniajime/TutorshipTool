@model WebApplication156456.Models.Sesion

@{
    ViewBag.Title = "PerfilEstudiante";
}

<html>
<head>
    <link href="~/css/PerfilEstudiante.css" rel="stylesheet" />
    <title>Perfil de estudiante</title>
</head>
<body>
    <br /><br /><br />
    <br />
    @if (ViewBag.thiserror == "true")
    {
        <div class="alert alert-success" role="alert">
            Se evaluó la sesión correctamente.
        </div>
    }
    <div class="container">
        <h1>Sesiones </h1>
    </div>
    <div id="exTab3" class="container">
        <ul class="nav nav-tabs">
            <li class="active">
                <a href="#1b" data-toggle="tab">Tutorías por venir</a>
            </li>
            <li>
                <a href="#2b" data-toggle="tab">En espera de confirmación</a>
            </li>
            <li>
                <a href="#3b" data-toggle="tab">Tutorías terminadas</a>
            </li>
        </ul>

        <div class="tab-content">
            <div class="tab-pane active" id="1b">

                @for (int index = 0; index < @ViewBag.sesions.Count; index++)
                {
                    if (ViewBag.sesions[index].estado_sesion == "Pendiente")
                    {
                        <div class="card" style="background-color: #f8efed;">
                            <div class="card-head" style="background-color: #17709e; color:white">
                                <h4 class="card-header text-center"><b>@ViewBag.sesions[index].nombre_curso</b></h4>
                            </div>
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-md-4"><b>Tutor</b></div>
                                    <div class="col-md-4"><b>Modalidad</b></div>
                                    <div class="col-md-4"><b>Cantidad de estudiantes</b></div>
                                </div>
                                <div class="row">
                                    <div class="col-md-4"><p class="card-text"> @ViewBag.sesions[index].nombre_tutor @ViewBag.sesions[index].apellido_tutor</p></div>
                                    <div class="col-md-4"><p class="card-text"> @ViewBag.sesions[index].modalidad</p></div>
                                    <div class="col-md-4"><p class="card-text"> @ViewBag.sesions[index].cantidad_estudiantes</p></div>
                                </div>
                                <div class="row">
                                    <div class="col-md-4"><b>Dirección</b></div>
                                    <div class="col-md-4"><b>Enlace de sesión</b></div>
                                    <div class="col-md-4"><b>Fecha de inicio:</b></div>
                                </div>
                                <div class="row">
                                    <div class="col-md-4"><p class="card-text">@ViewBag.sesions[index].direccion</p></div>
                                    <div class="col-md-4"><p class="card-text"><a href="@ViewBag.sesions[index].enlace">@ViewBag.sesions[index].enlace</a></p></div>
                                    <div class="col-md-4"><p class="card-text"> @ViewBag.sesions[index].fecha_inicio</p></div>
                                </div>
                                <div class="row">
                                    <div class="col-md-4"><b>Fecha de finalización</b></div>
                                    <div class="col-md-4"><b>Tarifa individual</b></div>
                                    <div class="col-md-4"><b>Tarifa grupal</b></div>
                                </div>
                                <div class="row">
                                    <div class="col-md-4"><p class="card-text"> @ViewBag.sesions[index].fecha_fin</p></div>
                                    <div class="col-md-4"><p class="card-text"> @ViewBag.sesions[index].tarifa_individual</p></div>
                                    <div class="col-md-4"><p class="card-text"> @ViewBag.sesions[index].tarifa_grupal</p></div>
                                </div>
                                <div class="row">
                                    <div class="col-md-4"><b>Estado de la sesion:</b></div>
                                    <div class="col-md-4"><b>Calificación de la sesión:</b></div>
                                    <div class="col-md-4"><b>Privacidad y contraseña de sesión:</b></div>
                                </div>
                                <div class="row">
                                    <div class="col-md-4"><p class="card-text"><b><span style="color:green">@ViewBag.sesions[index].estado_sesion</span></b></p></div>
                                    <div class="col-md-4">
                                        <p class="card-text">
                                            @for (int i = 0; i < ViewBag.sesions[index].evaluacion_sesion; i++)
                                            {
                                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-star-fill" viewBox="0 0 16 16">
                                                    <path d="M3.612 15.443c-.386.198-.824-.149-.746-.592l.83-4.73L.173 6.765c-.329-.314-.158-.888.283-.95l4.898-.696L7.538.792c.197-.39.73-.39.927 0l2.184 4.327 4.898.696c.441.062.612.636.282.95l-3.522 3.356.83 4.73c.078.443-.36.79-.746.592L8 13.187l-4.389 2.256z" />
                                                </svg>
                                            }
                                            @for (int i = ViewBag.sesions[index].evaluacion_sesion; i < 5; i++)
                                            {
                                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-star" viewBox="0 0 16 16">
                                                    <path d="M2.866 14.85c-.078.444.36.791.746.593l4.39-2.256 4.389 2.256c.386.198.824-.149.746-.592l-.83-4.73 3.522-3.356c.33-.314.16-.888-.282-.95l-4.898-.696L8.465.792a.513.513 0 0 0-.927 0L5.354 5.12l-4.898.696c-.441.062-.612.636-.283.95l3.523 3.356-.83 4.73zm4.905-2.767-3.686 1.894.694-3.957a.565.565 0 0 0-.163-.505L1.71 6.745l4.052-.576a.525.525 0 0 0 .393-.288L8 2.223l1.847 3.658a.525.525 0 0 0 .393.288l4.052.575-2.906 2.77a.565.565 0 0 0-.163.506l.694 3.957-3.686-1.894a.503.503 0 0 0-.461 0z" />
                                                </svg>
                                            }
                                        </p>
                                    </div>
                                    <div class="col-md-4"><p class="card-text">@ViewBag.sesions[index].privacidad  ||<span style="color:darkred"><b> @ViewBag.sesions[index].contrasena </b></span> </p></div>
                                </div>


                            </div>
                        </div>
                    }
                }

            </div>
            <div class="tab-pane" id="2b">
                @for (int index = 0; index < @ViewBag.sesions.Count; index++)
                {
                    if (ViewBag.sesions[index].estado_sesion == "Esperando Respuesta")
                    {

                        <div class="card" style="background-color: #f8efed;">
                            <div class="card-head" style="background-color: #17709e; color:white">
                                <h4 class="card-header text-center"><b>@ViewBag.sesions[index].nombre_curso</b></h4>
                            </div>
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-md-4"><b>Tutor</b></div>
                                    <div class="col-md-4"><b>Modalidad</b></div>
                                    <div class="col-md-4"><b>Cantidad de estudiantes</b></div>
                                </div>
